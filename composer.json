{
  "name": "koded/container",
  "type": "library",
  "homepage": "https://github.com/kodedphp/container",
  "description": "A simple Dependency Injection Container with application modules support",
  "license": "BSD-3-Clause",
  "keywords": [
    "dependency",
    "injection",
    "container",
    "psr-11",
    "dic"
  ],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "https://kodeart.com"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8",
    "psr/container": "^1"
  },
  "autoload": {
    "classmap": [
      ""
    ],
    "exclude-from-classmap": [
      "build/",
      "docs/",
      "Tests/"
    ]
  },
  "require-dev": {
    "ext-pdo": "*",
    "ext-json": "*",
    "phpunit/phpunit": "^9",
    "infection/infection": "@dev",
    "phpbench/phpbench": "@dev",
    "scrutinizer/ocular": "dev-master",
    "symfony/var-dumper": "^5"
  },
  "autoload-dev": {
    "psr-4": {
      "Koded\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}